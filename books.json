  "title": "First Platoon: The Pentagon's Biometric Mass Surveilance Horror",
  "info_link": "https://www.audible.com/pd/0593401999",
  "duration": "11:59",
  "author_link": "https://www.audible.com/author/Annie+Jacobsen/B001K7ZHF0",
  "narrated_by": "Annie Jacobsen",
  "product_id": "BK_PENG_005514",
