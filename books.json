  "title": "The Ghost - The Secret Life of CIA Spymaster James Jesus Angleton",
  "info_link": "https://www.audible.com/pd/B076R6SSC7",
  "duration": "09:53",
  "author_link": "https://www.audible.com/author/Jefferson+Morley/B001ILHJ9K",
